                    "title": "Number of Empty Receives, by Queue",
                    "rawConfiguration": {
                        "nrqlQueries": [
                            {
                                "accountId": 10662464,
                                "query": "SELECT sum(`aws.sqs.NumberOfEmptyReceives`) FROM Metric TIMESERIES facet `aws.sqs.QueueName`"
                            }
                        ]
                    }
                }
            ]
